{
  "functions": {
    "src/app/api/products/route.ts": {
      "maxDuration": 30,
      "memory": 1024
    },
    "src/app/api/products/[id]/route.ts": {
      "maxDuration": 30,
      "memory": 1024
    },
    "src/app/api/upload/route.ts": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "regions": ["iad1"],
  "framework": "nextjs",
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "production": true,
      "deploy": true
    }
  },
  "github": {
    "enabled": false
  },
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "devCommand": "npm run dev"
}